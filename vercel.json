{
  "version": 2,
  "builds": [
    {
      "src": "*.{js,css,html,json,webmanifest,xml}",
      "use": "@vercel/static"
    },
    {
      "src": "assets/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\\\.(css|js|json|webmanifest|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot|xml))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
