{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.(css|js|json|webmanifest|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot|xml))",
      "headers": { 
        "cache-control": "public, max-age=31536000, immutable",
        "content-type": "application/manifest+json"
      },
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
